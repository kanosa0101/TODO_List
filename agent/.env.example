# LLM Configuration
# Model ID (e.g., gpt-4, gpt-3.5-turbo, or other OpenAI-compatible models)
LLM_MODEL_ID=your-model-id

# API Key
LLM_API_KEY=your-api-key

# LLM Service URL (e.g., https://api.openai.com/v1 or other OpenAI-compatible services)
LLM_BASE_URL=https://api.openai.com/v1

# Request timeout in seconds (optional, default 60)
LLM_TIMEOUT=60

# Agent service port (optional, default 5000)
AGENT_PORT=5000